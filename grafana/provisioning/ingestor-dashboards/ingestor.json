{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "uid": "soam-ingestor-metrics",
  "panels": [
    {
      "type": "stat",
      "title": "Active Ingestor Pods",
      "description": "Number of ingestor pod replicas currently running",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "count(count by (pod) (ingestor_messages_received_total))",
          "refId": "A"
        }
      ],
      "gridPos": {"x": 0, "y": 0, "w": 4, "h": 4},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "yellow", "value": null},
              {"color": "green", "value": 1}
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "title": "Total Throughput",
      "description": "Messages per second across all pods",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(ingestor_messages_received_total[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "gridPos": {"x": 4, "y": 0, "w": 4, "h": 4},
      "fieldConfig": {
        "defaults": {
          "unit": "msg/s",
          "color": {"mode": "palette-classic"}
        }
      },
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value"
      }
    },
    {
      "type": "gauge",
      "title": "Success Rate",
      "description": "Percentage of messages successfully processed",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "100 * sum(rate(ingestor_messages_processed_total[$__rate_interval])) / sum(rate(ingestor_messages_received_total[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "gridPos": {"x": 8, "y": 0, "w": 4, "h": 4},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 90},
              {"color": "green", "value": 99}
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]}
      }
    },
    {
      "type": "stat",
      "title": "Data Rate",
      "description": "Bytes per second received",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(ingestor_bytes_received_total[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "gridPos": {"x": 12, "y": 0, "w": 4, "h": 4},
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "color": {"mode": "palette-classic"}
        }
      },
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value"
      }
    },
    {
      "type": "stat",
      "title": "Files Written/s",
      "description": "Parquet files written to MinIO per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(ingestor_files_written_total[$__rate_interval]))",
          "refId": "A"
        }
      ],
      "gridPos": {"x": 20, "y": 0, "w": 4, "h": 4},
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {"mode": "palette-classic"}
        }
      },
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "colorMode": "value"
      }
    },
    {
      "type": "timeseries",
      "title": "Messages Received (Total & Per Pod)",
      "description": "Aggregate and per-pod message ingestion rate",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(ingestor_messages_received_total[$__rate_interval]))",
          "legendFormat": "Total",
          "refId": "A"
        },
        {
          "expr": "rate(ingestor_messages_received_total[$__rate_interval])",
          "legendFormat": "{{pod}}",
          "refId": "B"
        }
      ],
      "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {
          "unit": "msg/s",
          "color": {"mode": "palette-classic"}
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "type": "timeseries",
      "title": "Messages by Source Type",
      "description": "Breakdown of messages by data source type (MQTT, REST, etc.)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (source_type) (rate(ingestor_messages_received_total[$__rate_interval]))",
          "legendFormat": "{{source_type}}",
          "refId": "A"
        }
      ],
      "gridPos": {"x": 0, "y": 12, "w": 8, "h": 6},
      "fieldConfig": {
        "defaults": {
          "unit": "msg/s",
          "color": {"mode": "palette-classic"}
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "type": "timeseries",
      "title": "Messages by Ingestion ID",
      "description": "Breakdown by data source/topic",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (ingestion_id) (rate(ingestor_messages_received_total[$__rate_interval]))",
          "legendFormat": "{{ingestion_id}}",
          "refId": "A"
        }
      ],
      "gridPos": {"x": 8, "y": 12, "w": 8, "h": 6},
      "fieldConfig": {
        "defaults": {
          "unit": "msg/s",
          "color": {"mode": "palette-classic"}
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate by Type",
      "description": "Processing errors by error type",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum by (error_type) (rate(ingestor_messages_failed_total[$__rate_interval]))",
          "legendFormat": "{{error_type}}",
          "refId": "A"
        }
      ],
      "gridPos": {"x": 0, "y": 18, "w": 12, "h": 6},
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {"mode": "palette-classic"}
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    },
    {
      "type": "timeseries",
      "title": "Data Volume (Bytes Received)",
      "description": "Total bytes ingested per second",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(ingestor_bytes_received_total[$__rate_interval]))",
          "legendFormat": "Total Bytes/s",
          "refId": "A"
        },
        {
          "expr": "sum by (source_type) (rate(ingestor_bytes_received_total[$__rate_interval]))",
          "legendFormat": "{{source_type}}",
          "refId": "B"
        }
      ],
      "gridPos": {"x": 12, "y": 18, "w": 12, "h": 6},
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "color": {"mode": "palette-classic"}
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"}
      }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "tags": ["soam", "ingestor", "prometheus", "metrics"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "title": "Ingestor Metrics Dashboard",
  "version": 2,
  "weekStart": ""
}